<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE subjectScheme PUBLIC "-//OASIS//DTD DITA Subject Scheme Map//EN" "subjectScheme.dtd"> 
<subjectScheme>
  <subjectHead>
    <subjectHeadMeta>
      <navtitle>Subject Scheme</navtitle>
    </subjectHeadMeta>
  </subjectHead>
  <hasInstance>
    <subjectdef keys="subjects">
      <topicmeta>
        <navtitle>Subjects</navtitle>
      </topicmeta>
      <subjectdef keys="subject-a">
        <topicmeta>
          <navtitle>Subject A</navtitle>
        </topicmeta>
      </subjectdef>
      <subjectdef keys="subject-b">
        <topicmeta>
          <navtitle>Subject B</navtitle>
        </topicmeta>
      </subjectdef>
    </subjectdef>
  </hasInstance>
  <enumerationdef>
    <!-- Bind an attribute to a tree of related values -->
    <attributedef name="otherprops"/>
    <subjectdef keyref="subjects"/>
  </enumerationdef>
</subjectScheme>
